{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "SystemInfoDb": "Server=YOUR_SERVER_NAME;Database=YOUR_DB_NAME;Trusted_Connection=True;TrustServerCertificate=True"
  },
  "DatabaseConfig": {
    "TablesNames": {
      "CustomerTableName": "Customer", // The name of YOUR ALREADY EXISTING table containing your customers in your database.
      "MachineTableName": "Customer_Machine",
      "DriveTableName": "Customer_Machine_Drive",
      "OsTableName": "Customer_Machine_Drive_Os"
    },
    "MachineTableColumns": {
      "MachineId": "id_customer_machine",
      "MachineCustomerId": "id_customer",
      "MachineName": "Name"
    },
    "DriveTableColumns": {
      "DriveId": "id_customer_machine_drive",
      "DriveMachineId": "id_customer_machine",
      "DriveName": "Name",
      "DriveRootDirectory": "Root_Directory",
      "DriveLabel": "Label",
      "DriveType": "Type",
      "DriveFormat": "Format",
      "DriveSize": "Size",
      "DriveFreeSpace": "Free_Space",
      "DriveTotalSpace": "Total_Space",
      "DriveFreeSpacePercentage": "Free_Space_Percentage",
      "DriveIsSystemDrive": "Is_System_Drive"
    },
    "OsTableColumns": {
      "OsId": "id_customer_machine_drive_os",
      "OsDriveId": "id_customer_machine_drive",
      "OsDirectory": "Directory",
      "OsArchitecture": "Architecture",
      "OsVersion": "Version",
      "OsProductName": "Product_Name",
      "OsReleaseId": "Release_Id",
      "OsCurrentBuild": "Current_Build",
      "OsUbr": "Ubr"
    }
  },
  "AllowedHosts": "*"
}
