{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "SystemInfoDb": "Server=YOUR_SERVER_NAME;Database=YOUR_DB_NAME;Trusted_Connection=True;TrustServerCertificate=True"
  },
  "DatabaseConfig": {
    "TablesNames": {
      "CustomersTable": "Customer", // The name of the already existing table containing the customers in your database.
      "MachinesTable": "Customer_Machine",
      "DrivesTable": "Customer_Machine_Drive",
      "OsTable": "Customer_Machine_Drive_Os"
    }
  },
  "AllowedHosts": "*"
}
